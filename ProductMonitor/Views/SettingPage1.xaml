<Page x:Class="ProductMonitor.Views.SettingPage1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ProductMonitor.Views"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="SettingPage1">

    <Grid >
        <Grid.RowDefinitions>
            <RowDefinition Height="30"></RowDefinition>
            <RowDefinition  Height="auto"></RowDefinition>
            <RowDefinition Height="30"></RowDefinition>
            <RowDefinition Height="auto"></RowDefinition>
            <RowDefinition Height="30"></RowDefinition>
            <RowDefinition Height="auto"></RowDefinition>
            <RowDefinition Height="30"></RowDefinition>
            <RowDefinition Height="auto"></RowDefinition>
            <RowDefinition Height="30"></RowDefinition>
            <RowDefinition Height="auto"></RowDefinition>
        </Grid.RowDefinitions>
        <Border BorderBrush="#eee" BorderThickness="0,0,0,1"></Border>
        <Border Grid.Row="2" BorderBrush="#eee" BorderThickness="0,0,0,1"></Border>
        <Border Grid.Row="4" BorderBrush="#eee" BorderThickness="0,0,0,1"></Border>
        <Border Grid.Row="6" BorderBrush="#eee" BorderThickness="0,0,0,1"></Border>
        <Border Grid.Row="8" BorderBrush="#eee" BorderThickness="0,0,0,1"></Border>
        <Label Content="设备清单" VerticalAlignment="Center" Name="s1"></Label>
        <Label Content="环境监测" VerticalAlignment="Center"  Grid.Row="2" Name="s2"></Label>
        <Label Content="配置监测" VerticalAlignment="Center"  Grid.Row="4" Name="s3"></Label>
        <Label Content="配置一" VerticalAlignment="Center"  Grid.Row="6" Name="s4"></Label>
        <Label Content="配置二" VerticalAlignment="Center"  Grid.Row="8" Name="s5"></Label>
        
    </Grid>
</Page>
