<UserControl x:Class="ProductMonitor.Usercontrol.RaderUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ProductMonitor.Usercontrol"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid x:Name="Rader">
            <!--画布-->
        <Canvas x:Name="Maincanvas"></Canvas>
        <!--背景多边形-->
        <Polygon x:Name="p1" Stroke="#22ffffff"></Polygon>
        <Polygon x:Name="p2" Stroke="#22ffffff"></Polygon>
        <Polygon x:Name="p3" Stroke="#22ffffff"></Polygon>
        <Polygon x:Name="p4" Stroke="#22ffffff"></Polygon>
        <!--数据多边形-->
        <Polygon x:Name="p5" Stroke="pink" Fill="#550091f0"></Polygon>
    </Grid>
</UserControl>
